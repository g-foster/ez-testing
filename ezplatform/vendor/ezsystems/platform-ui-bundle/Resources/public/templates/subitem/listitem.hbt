{{#each properties}}
    {{#if rendered}}
        {{{ rendered }}}
    {{else}}
        <td class="{{ class }}">{{ value }}</td>
    {{/if}}
{{/each}}
<td class="ez-subitemlistitem-cell">
    <button class="ez-subitemlistitem-edit ez-font-icon pure-button ez-button">
        {{translate 'subitem.listitem.edit' 'subitem'}}
    </button>
</td>
