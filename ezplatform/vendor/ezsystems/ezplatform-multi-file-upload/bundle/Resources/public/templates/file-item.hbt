<div class="mfu-file-item__meta">
    <div class="mfu-file-item__thumb">
        <svg class="mfu-file-item__icon mfu-file-item__icon-{{ type }}">
            <use xlink:href="#mfu-icon-{{ type }}"></use>
        </svg>
    </div>
    <div class="mfu-file-item__info">
        <div class="mfu-file-item__filename">{{ filename }}</div>
        <div class="mfu-file-item__upload-state--done mfu--hidden">{{ fileUploadDoneText }}</div>
        <div class="mfu-file-item__upload-state--error mfu--hidden"></div>
        <div class="mfu-file-item__upload-state--in-progress">
            <div class="mfu-file-item__progress-bar">
                <div class="mfu-file-item__progress-value" style="width: 0;"></div>
            </div>
            <div class="mfu-file-item__progress-status">{{ progressStatus }}</div>
            <div class="mfu-file-item__upload-status">{{ fileUploadStatusText }}</div>
        </div>
    </div>
    <div class="mfu-file-item__options">
        <button class="mfu-file-item__option mfu--hidden" data-option="edit">
            <svg class="mfu-file-item__icon mfu-file-item__icon-edit">
                <use xlink:href="#mfu-icon-edit"></use>
            </svg>
        </button>
        <button class="mfu-file-item__option mfu--hidden" data-option="remove">
            <svg class="mfu-file-item__icon mfu-file-item__icon-remove">
                <use xlink:href="#mfu-icon-remove"></use>
            </svg>
        </button>
        <button class="mfu-file-item__option" data-option="abort">
            <svg class="mfu-file-item__icon mfu-file-item__icon-abort">
                <use xlink:href="#mfu-icon-abort"></use>
            </svg>
        </button>
    </div>
</div>
<div class="mfu-file-item__error mfu--hidden"></div>
